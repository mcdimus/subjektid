<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
	<display-name>JSP/Servlet/JavaBean kombinatsioon</display-name>
	<description>
		servlet/bean
	</description>
	
	<servlet>
		<servlet-name>
			FrontController
		</servlet-name>
		<servlet-class>
			frontend.control.FrontController
		</servlet-class>

	</servlet>

	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>/s</url-pattern>
	</servlet-mapping>
</web-app>
